[org.gnome.desktop.interface:MATE]
cursor-theme='mate'
document-font-name='Cantarell 11'
font-name='Cantarell 11'
gtk-theme='BlackMATE'
icon-theme='menta'
monospace-font-name='Monospace 13'

[org.gnome.desktop.wm.preferences:MATE]
